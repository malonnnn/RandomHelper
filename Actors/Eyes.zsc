class SummonedBatFamiliar : Actor
{
  Default
  {
    MONSTER;
    +FRIENDLY
    Species "Companion";
    DamageFactor "Companion", 0;
    Health 10;
    Radius 12;
    Height 24;
    Speed 14;
    PainChance 200;
    +NOGRAVITY
    +FLOAT
    +NEVERRESPAWN
    SeeSound "batfam/idle";
    PainSound "batfam/pain";
    DeathSound "batfam/death";
    ActiveSound "batfam/idle";
    HitObituary "%o was scratched by a bat familiar";
  }
  States
  {
    Spawn:
      BFAM ABCB 3 A_Look;
      Loop;
    See:
      BFAM ABCB 3 A_Chase;
      Loop;
    Melee:
      TNT1 A 0 A_AlertMonsters(0, AMF_TARGETEMITTER);
      BFAM A 3 A_FaceTarget;
      BFAM B 3 A_CustomMeleeAttack(Random(1,8)*1,"batfam/idle", "", "Companion");
      BFAM CB 3 A_FaceTarget;
      Goto See;
    Pain:
      BFAM A 2;
      BFAM A 2 A_Pain;
      Goto See;
    Death:
      TNT1 A 0 A_Die;
      TNT1 A 0 A_NoBlocking;
      BFAM D 5 A_Scream;
      BFAM E 5;
      BFAM FDEF 5;
      Goto Death+5;
    Crash:
	    TNT1 A 0 A_SetFloorClip;
      BFAM G 6 A_StartSound("batfam/crash");
      BFAM H 7;
      BFAM I 8;
      BFAM J -1;
      Stop;
  }
}