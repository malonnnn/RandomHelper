Class SpiderSpawner : Actor //Projectile that makes spider
{
  Default
  {
    Species "Companion";
    DamageType "Companion";
    Radius 16;
    Height 16;
    Speed 6;
    Scale 0.2;
    PROJECTILE;
    +FLOAT
    +BLOODLESSIMPACT
    +PAINLESS
    +RIPPER
  }
	States
	{
    Spawn:
      TNT1 A 10;
    Death:
      TNT1 A 1
