Class EyeEx : CompanionLostSoul //Just the normal explosive one
{
  Default
  {
    MONSTER;
    +FRIENDLY
    Species "Companion";
    DamageFactor "Companion", 0;
    Obituary "%o was destroyed by an Exploding Eye.";
    Damage 0;
    Translation "112:127=176:191";
    -DONTFALL
    +LOOKALLAROUND
  }
	States
  {
  	Spawn:
	  	ESYM AB 10 bright A_Look;
   		Loop;
  	See:
      ESYM ABCD 6 bright A_Chase;
      Loop;
  	Missile:
      ESYM C 10 bright A_FaceTarget;
      ESYM D 2 bright A_SkullAttack;
      TNT1 A 0 A_AlertMonsters(0, AMF_TARGETEMITTER);
      ESYM CCCCDDDD 1 bright
      {
        //A_JumpIfCloser(64, "Melee");
        if(target && Distance3D(target) <= 64 && !(target is "PatrolPoint")){
          SetStateLabel("Melee");
        }
      }
      Goto Missile+2;
	  Melee:
      TNT1 A 0 A_AlertMonsters(0, AMF_TARGETEMITTER);
  		TNT1 A 0 A_Die;
    Pain:
      ESYM F 3 bright;
      ESYM F 3 bright A_Pain;
      Goto See;
  	Death:
      ESYM O 4 bright;
      ESYM P 4 bright A_Scream;
      TNT1 A 0 A_NoBlocking;
      ESYM Q 4 bright A_Explode(100,128);
      ESYM R 4 bright;
      ESYM STUV 4 A_Gravity;
      ESYM W -1;
      Stop;
  }
}

