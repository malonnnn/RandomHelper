      }
      Stop;
	}
}

Class Spider : Actor //Spiders from Spider Eye, sprites ripped from Blood by Psyren
{	
  Default
  {
  	MONSTER;
    +FRIENDLY
    Species "Companion";
    DamageFactor "Companion", 0;
    Health 20;
    Radius 64;
    Height 64;
  	Mass 20;
  	Speed 12;
  	PainChance 128;
  	SeeSound "SpiderSee";
  	PainSound "baby/pain";
  	DeathSound "SpiderDie";
  	ActiveSound "SpiderSee";
    MeleeRange 16;
    Radius 8;
    Height 8;
    Scale 0.25;
  	+FLOORCLIP
    +NEVERRESPAWN
    Obituary"%o was webbed by a Spider.";
  }
	States
 	{
    Spawn:
      ZPID AB 10 A_Look;
      Loop;
    See:
      ZPID A 20;
      ZPID AABBCCDDEEFF 3 A_Chase;
      Goto See+1;
    Melee:
      ZPID A 4 A_FaceTarget;
      TNT1 A 0 A_AlertMonsters(0, AMF_TARGETEMITTER);
      ZPID G 4 A_CustomMeleeAttack(Random(1,8)*2);
      ZPID H 4;
      Goto See+1;
    Pain:
      ZPID I 3;
      ZPID I 3 A_Pain;
      Goto See+1;
    Death:
      ZPID J 2 A_Scream;
      ZPID K 2 A_NoBlocking;
      ZPID LMN 2;
      ZPID O -1;
      Stop;
    Raise:
      ZPID ONMLKJ 5;
      Goto See+1;
  }
}

